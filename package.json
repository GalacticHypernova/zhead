{
  "name": "zhead-monorepo",
  "private": "true",
  "version": "0.0.2",
  "packageManager": "pnpm@7.8.0",
  "description": "Node Schema.org for Simple and Automated Google Rich Results",
  "author": "Harlan Wilton <harlan@harlanzw.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://github.com/harlan-zw/zhead#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/zhead.git"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/zhead/issues"
  },
  "keywords": [
    "meta tags",
    "node",
    "seo"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm -r --filter=./packages/** run build",
    "stub": "pnpm -r --parallel --filter=./packages/** run stub",
    "test": "vitest",
    "export:sizes": "cd packages/zhead && npx export-size . -r",
    "release": "bumpp package.json --commit --push --tag",
    "lint": "eslint \"{packages,test}/**/*.{ts,vue,json,yml}\" --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.2",
    "@types/fs-extra": "^9.0.13",
    "@vitest/ui": "^0.22.1",
    "bumpp": "^8.2.1",
    "eslint": "^8.23.0",
    "fs-extra": "^10.1.0",
    "jsdom": "^20.0.0",
    "typescript": "^4.8.2",
    "unbuild": "^0.8.9",
    "utility-types": "^3.10.0",
    "vitest": "^0.22.1"
  }
}
